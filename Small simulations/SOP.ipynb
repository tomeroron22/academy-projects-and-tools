    "a = al(s1, s3,m, miss, g,g)[0]\n",
    "print(f\"{a.seqA}\\n{a.seqB}\\nscore:{a.score}\\n\")\n",
    "a = al(s2,s3,m, miss, g,g)[0]\n",
